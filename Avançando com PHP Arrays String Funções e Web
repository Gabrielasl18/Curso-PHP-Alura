- parte 1:

* Arrays associativos:

<?php
$conta1 = [
  'titular' => 'Gabriela',
  'saldo' => 1000
];
$conta2 = [
  'titular' => 'Maria',
  'saldo' => 10000
];
$conta3 = [
  'titular' => 'Alberto',
  'saldo' => 300
];

$contasCorrentes = [$conta1, $conta2, $conta3];

for($i = 0; $i < count($contasCorrentes); $i++) {
echo $contasCorrentes[$i]['titular'] . "\n";
  
}
